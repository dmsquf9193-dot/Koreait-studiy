-- INNER JOIN - 교집합의 값만 조회
-- 사번, 이름, 연봉 조회
SELECT e.emp_no, e.first_name, s.salary 
FROM employees e
JOIN salaries s 
ON e.emp_no = s.emp_no 
WHERE s.to_date = '9999-01-01';

-- 다 가지고 오는 것
SELECT *
FROM employees e
JOIN salaries s 
ON e.emp_no = s.emp_no 
WHERE s.to_date = '9999-01-01';

-- 사번, 이름, 직무 조회 (헌재 재직중인)
SELECT emp_no, first_name, title
FROM employees e 
JOIN titles t 
NO e.emp_no = t. title
WHERE t. 

